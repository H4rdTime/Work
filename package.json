    "@types/react": "^19.0.10",
    "@types/react-dom": "^19",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.1.6",
    "lighthouse": "^12.4.0",
    "postcss": "^8",
    "sharp": "^0.33.5",
    "supabase": "^2.15.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
