    "@types/react-input-mask": "^3.0.6",
    "axios": "^1.7.9",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "embla-carousel-wheel-gestures": "^8.0.1",
    "image-utils": "^1.0.0",
    "lodash": "^4.17.21",
    "next": "^15.2.2",
    "next-sitemap": "^4.2.3",
    "plaiceholder": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-imask": "^7.6.1",
    "react-input-mask": "^2.0.4",
    "react-intersection-observer": "^9.16.0",
